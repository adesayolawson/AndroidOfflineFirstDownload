<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.downloadmanagersample.FirstFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_first">

        <action
            android:id="@+id/action_FirstFragment_to_exoPlayerActivity"
            app:destination="@id/exoPlayerActivity" />
    </fragment>
    <activity
        android:id="@+id/exoPlayerActivity"
        android:name="com.example.downloadmanagersample.exoplayer.ExoPlayerActivity"
        android:label="activity_exo_player"
        tools:layout="@layout/activity_exo_player" >
        <argument
            android:name="fileSync"
            app:argType="com.example.downloadmanagersample.database.model.fileSync.FileSync" />
    </activity>
</navigation>